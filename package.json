{
  "name": "gupo-naive-admin",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "new:page": "plop --plopfile scripts/create-page.mjs ",
    "dev": "vite --host --port 63001",
    "serve": "dev",
    "serve:stage": "vite --mode staging",
    "serve:prod": "vite --mode production",
    "build-only": "vite build --mode development",
    "build": "run-p type-check build-only",
    "build:stage": "vite build --mode staging",
    "build:prod": "vite build --mode production",
    "deploy": "gupo-deploy --mode staging -p --env_dir ./env",
    "deploy:prod": "gupo-deploy --mode production -p --env_dir ./env",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint ./src --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "lint:stylelint": "stylelint --fix './src/**/*.{css,scss,less,vue}' --cache --cache-location node_modules/.cache/stylelint/",
    "preview": "vite preview",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "lint-staged": {
    "src/**/*.{vue,ts,tsx,scss,css}": [
      "prettier --write "
    ],
    "src/**/*.{vue,ts,tsx}": [
      "npm run lint"
    ],
    "src/**/*.{scss,vue}": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "@unocss/reset": "^0.55.4",
    "@vicons/ionicons5": "^0.12.0",
    "@vueuse/core": "^10.4.1",
    "axios": "^1.5.0",
    "echarts": "^5.4.3",
    "lodash-unified": "^1.0.3",
    "markdown-it-prism": "^2.3.0",
    "markdown-it-table-of-contents": "^0.6.0",
    "monaco-editor": "^0.41.0",
    "naive-ui": "^2.34.4",
    "pinia": "^2.1.6",
    "prism-theme-vars": "^0.2.4",
    "unplugin-vue-markdown": "^0.24.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@iconify-json/line-md": "^1.1.29",
    "@iconify/json": "^2.2.109",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.1",
    "@types/node": "^20.5.7",
    "@unocss/eslint-config": "^0.55.4",
    "@vicons/antd": "^0.12.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/shared": "^3.3.4",
    "@vue/tsconfig": "^0.4.0",
    "chalk": "^5.3.0",
    "eslint": "^8.48.0",
    "eslint-plugin-vue": "^9.17.0",
    "gupo-deploy": "^1.3.0",
    "husky": "^8.0.3",
    "inquirer-search-list": "^1.2.6",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "plop": "^4.0.0",
    "postcss-html": "^1.5.0",
    "prettier": "^3.0.3",
    "sass": "^1.66.1",
    "stylelint": "^15.10.3",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-recommended-scss": "^13.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-prettier": "^4.0.2",
    "stylelint-scss": "^5.2.1",
    "typescript": "~5.2.2",
    "unocss": "^0.55.4",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.9",
    "vite-plugin-compression2": "^0.10.4",
    "vite-plugin-env-types": "^0.1.3",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "1.0.0-rc.3",
    "vue-tsc": "^1.8.8"
  }
}
