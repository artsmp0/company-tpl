{
  "name": "gupo-naive-admin",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "serve": "vite",
    "serve:stage": "vite --mode staging",
    "serve:prod": "vite --mode production",
    "preview": "vite preview",
    "build-only": "vite build --mode development",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "lint:style": "stylelint '**/*.{scss,vue}' --fix",
    "prepare": "husky install",
    "build": "run-p type-check build-only",
    "build:stage": "vite build --mode staging",
    "build:prod": "vite build --mode production",
    "deploy": "gupo-deploy --mode staging -p --env_dir ./env",
    "deploy:prod": "gupo-deploy --mode production -p --env_dir ./env"
  },
  "lint-staged": {
    "src/**/*.{vue,ts,tsx,scss,css}": [
      "prettier --write "
    ],
    "src/**/*.{vue,ts,tsx}": [
      "npm run lint"
    ],
    "src/**/*.{scss,vue}": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "@unocss/reset": "^0.55.4",
    "@vicons/ionicons5": "^0.12.0",
    "axios": "^1.5.0",
    "echarts": "^5.4.3",
    "lodash-unified": "^1.0.3",
    "monaco-editor": "^0.41.0",
    "naive-ui": "^2.34.4",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@iconify-json/line-md": "^1.1.29",
    "@iconify/json": "^2.2.109",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.1",
    "@types/node": "^20.5.7",
    "@unocss/eslint-config": "^0.55.4",
    "@vicons/antd": "^0.12.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "@vueuse/core": "^10.4.1",
    "eslint": "^8.48.0",
    "eslint-plugin-vue": "^9.17.0",
    "gupo-deploy": "^1.3.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-html": "^1.5.0",
    "prettier": "^3.0.3",
    "sass": "^1.66.1",
    "stylelint": "^15.10.3",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-prettier": "^4.0.2",
    "typescript": "~5.2.2",
    "unocss": "^0.55.4",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.9",
    "vite-plugin-env-types": "^0.1.3",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^1.8.8"
  }
}
