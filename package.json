{
  "name": "gupo-naive-admin",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite",
    "serve": "vite",
    "serve:stage": "vite --mode staging",
    "serve:prod": "vite --mode production",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "lint:style": "stylelint '**/*.{scss,vue}' --fix",
    "prepare": "husky install",
    "build": "run-p type-check build-only",
    "build:stage": "vite build --mode staging",
    "build:prod": "vite build --mode production",
    "deploy": "gupo-deploy --mode staging -p --env_dir ./env",
    "deploy:prod": "gupo-deploy --mode production -p --env_dir ./env"
  },
  "lint-staged": {
    "src/**/*.{vue,ts,tsx,scss,css}": [
      "prettier --write "
    ],
    "src/**/*.{vue,ts,tsx}": [
      "npm run lint"
    ],
    "src/**/*.{scss,vue}": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "@unocss/reset": "^0.53.1",
    "@vicons/ionicons5": "^0.12.0",
    "axios": "^1.4.0",
    "echarts": "^5.4.2",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.4",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^20.3.1",
    "@unocss/eslint-config": "^0.53.1",
    "@vicons/antd": "^0.12.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "@vueuse/core": "^10.1.2",
    "eslint": "^8.42.0",
    "eslint-plugin-vue": "^9.14.1",
    "gupo-deploy": "^1.3.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "naive-ui": "^2.34.4",
    "npm-run-all": "^4.1.5",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.8",
    "sass": "^1.63.4",
    "stylelint": "^15.7.0",
    "stylelint-config-recess-order": "^4.2.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-prettier": "^3.0.0",
    "typescript": "~5.1.3",
    "unocss": "^0.53.1",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.3.9",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^1.6.5"
  }
}
