{
    "name": "gupo-naive-admin",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "new:page": "plop --plopfile scripts/create-page.mjs ",
        "dev": "vite --host --port 63001",
        "serve": "dev",
        "serve:stage": "vite --mode staging",
        "serve:prod": "vite --mode production",
        "build-only": "vite build --mode development",
        "build": "run-p type-check build-only",
        "build:stage": "vite build --mode staging",
        "build:prod": "vite build --mode production",
        "deploy": "gupo-deploy --mode staging -p --env_dir ./env",
        "deploy:prod": "gupo-deploy --mode production -p --env_dir ./env",
        "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
        "lint:eslint": "eslint ./src --fix",
        "lint:stylelint": "stylelint --fix './src/**/*.{css,scss,less,vue}' --cache --cache-location node_modules/.cache/stylelint/",
        "lint:prettier": "prettier . --write  --ignore-unknown",
        "lint:ls-lint": "ls-lint",
        "preview": "vite preview",
        "prepare": "husky install",
        "preinstall": "npx only-allow pnpm"
    },
    "dependencies": {
        "@unocss/reset": "^0.56.5",
        "@vicons/ionicons5": "^0.12.0",
        "@vueuse/core": "^10.4.1",
        "axios": "^1.5.1",
        "echarts": "^5.4.3",
        "lodash-unified": "^1.0.3",
        "markdown-it-prism": "^2.3.0",
        "markdown-it-table-of-contents": "^0.6.0",
        "monaco-editor": "^0.44.0",
        "naive-ui": "^2.35.0",
        "pinia": "^2.1.6",
        "prism-theme-vars": "^0.2.4",
        "unplugin-vue-markdown": "^0.24.3",
        "vue": "^3.3.4",
        "vue-router": "^4.2.5"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.7.2",
        "@commitlint/config-conventional": "^17.7.0",
        "@iconify-json/line-md": "^1.1.29",
        "@iconify/json": "^2.2.125",
        "@ls-lint/ls-lint": "^2.1.0",
        "@rushstack/eslint-patch": "^1.5.1",
        "@tsconfig/node18": "^18.2.2",
        "@types/node": "^20.8.3",
        "@unocss/eslint-config": "^0.56.5",
        "@vicons/antd": "^0.12.0",
        "@vitejs/plugin-vue": "^4.4.0",
        "@vitejs/plugin-vue-jsx": "^3.0.2",
        "@vue/eslint-config-prettier": "^8.0.0",
        "@vue/eslint-config-typescript": "^12.0.0",
        "@vue/shared": "^3.3.4",
        "@vue/tsconfig": "^0.4.0",
        "chalk": "^5.3.0",
        "eslint": "^8.51.0",
        "eslint-plugin-vue": "^9.17.0",
        "gupo-deploy": "^1.4.0",
        "husky": "^8.0.3",
        "inquirer-search-list": "^1.2.6",
        "lint-staged": "^14.0.1",
        "npm-run-all": "^4.1.5",
        "plop": "^4.0.0",
        "postcss-html": "^1.5.0",
        "prettier": "^3.0.3",
        "sass": "^1.69.0",
        "stylelint": "^15.10.3",
        "stylelint-config-recess-order": "^4.3.0",
        "stylelint-config-recommended": "^13.0.0",
        "stylelint-config-recommended-scss": "^13.0.0",
        "stylelint-config-recommended-vue": "^1.5.0",
        "stylelint-prettier": "^4.0.2",
        "stylelint-scss": "^5.2.1",
        "typescript": "~5.2.2",
        "unocss": "^0.56.5",
        "unplugin-auto-import": "^0.16.6",
        "unplugin-vue-components": "^0.25.2",
        "vite": "^4.4.11",
        "vite-plugin-compression2": "^0.10.5",
        "vite-plugin-env-types": "^0.1.3",
        "vite-plugin-svg-icons": "^2.0.1",
        "vite-plugin-vue-devtools": "1.0.0-rc.5",
        "vue-tsc": "^1.8.16"
    }
}
